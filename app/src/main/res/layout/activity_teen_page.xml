<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Teen_page">

    <TextView
        android:id="@+id/chitchat"
        android:layout_width="280dp"
        android:layout_height="166dp"
        android:layout_marginTop="96dp"
        android:fontFamily="sans-serif-black"
        android:gravity="center"
        android:onClick="text_msg"
        android:text="Chit Chat \n with Maria Gonzalas \n from 4:00pm - 5:00pm \n click to join"
        android:textSize="26sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.442"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/talentshow"
        android:layout_width="280dp"
        android:layout_height="166dp"
        android:layout_marginTop="296dp"
        android:fontFamily="sans-serif-black"
        android:gravity="center"
        android:onClick="text_msg"
        android:text="Talent Show \n with Maven Brown \n from 6:00pm - 7:00pm \n click to join"
        android:textSize="26sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.442"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/paint"
        android:layout_width="280dp"
        android:layout_height="166dp"
        android:layout_marginTop="496dp"
        android:fontFamily="sans-serif-black"
        android:gravity="center"
        android:onClick="text_msg"
        android:text="Paint with Peers \n with Ashley Tran \n from 6:00pm - 7:00pm \n click to join"
        android:textSize="26sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.442"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


</androidx.constraintlayout.widget.ConstraintLayout>