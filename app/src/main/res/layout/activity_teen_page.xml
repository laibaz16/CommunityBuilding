<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Teen_page">




    <Button
        android:id="@+id/talentshow"
        android:layout_width="126dp"
        android:layout_height="94dp"
        android:layout_marginTop="56dp"
        android:text="Talent Show"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.223"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Chitchat" />

    <Button
        android:id="@+id/Paint"
        android:layout_width="152dp"
        android:layout_height="105dp"
        android:text="Paint with Peers"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.223"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/talentshow"
        app:layout_constraintVertical_bias="0.159" />

    <Button
        android:id="@+id/Chitchat"
        android:layout_width="119dp"
        android:layout_height="106dp"
        android:layout_marginTop="184dp"
        android:text="Chit Chat with Host Maria Gonzalas "

        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.219"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="105dp"
        android:layout_height="68dp"
        android:text="Chit Chat with Maria Gonzalas from 4:00pm - 5:00pm"
        android:onClick="text_msg"
        app:layout_constraintBottom_toTopOf="@+id/Chitchat"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.209"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.629" />


</androidx.constraintlayout.widget.ConstraintLayout>